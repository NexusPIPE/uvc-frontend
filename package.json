{
  "name": "@nexusuvc/frontend",
  "version": "1.0.1",
  "description": "NexusUVC Svelte & Plain HTML Widget",
  "keywords": [
    "nexuspipe",
    "nexusuvc",
    "uvc",
    "captcha",
    "widget",
    "svelte",
    "sveltekit",
    "html"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && vite -c vite.plain.config.ts build && cp dist/index.d.ts vanilla/index.d.ts && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./vanilla": {
      "types": "./dist/index.d.ts",
      "require": "./vanilla/index.cjs",
      "import": "./vanilla/index.js",
      "svelte": "./vanilla/index.js",
      "default": "./vanilla/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "vanilla"
  ],
  "dependencies": {
    "json5": "^2.2.3"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.27.6",
    "@sveltejs/package": "^2.2.3",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "publint": "^0.2.6",
    "sass": "^1.69.5",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "vite": "^5"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
